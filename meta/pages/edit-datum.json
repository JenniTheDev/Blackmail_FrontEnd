{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "User Id",
                              "component": "UserID",
                              "name": "UserIDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int",
                              "type": "numeric",
                              "name": "UserID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${datum.UserID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "UserIDRequiredValidator",
                              "component": "UserID",
                              "text": "UserID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Blackmailer",
                              "component": "Blackmailer",
                              "name": "BlackmailerLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 45,
                              "type": "textbox",
                              "name": "Blackmailer",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${datum.Blackmailer}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Blackmailee",
                              "component": "Blackmailee",
                              "name": "BlackmaileeLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 45,
                              "type": "textbox",
                              "name": "Blackmailee",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${datum.Blackmailee}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Cost",
                              "component": "cost",
                              "name": "costLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "double?",
                              "type": "numeric",
                              "name": "cost",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${datum.cost}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Img",
                              "component": "img",
                              "name": "imgLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 45,
                              "type": "textbox",
                              "name": "img",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${datum.img}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Datum",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "blackmailazure.updateDatum",
                        "parameters": [
                          {
                            "name": "UserID",
                            "value": "${parameters.UserID}"
                          },
                          {
                            "name": "Datum",
                            "value": "${datum}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${datum}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Blackmail.Models.Blackmailazure.Datum",
                  "data": "${datum}",
                  "visible": "${datum != null}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "blackmailazure.getDatumByUserID",
        "parameters": [
          {
            "name": "UserID",
            "value": "${parameters.UserID}"
          }
        ],
        "then": [
          {
            "name": "datum",
            "type": "set",
            "value": "${result}",
            "valueType": "Blackmail.Models.Blackmailazure.Datum"
          }
        ],
        "error": [],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Datum",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}