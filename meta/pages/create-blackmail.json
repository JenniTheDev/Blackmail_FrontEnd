                      "type": "row"
                    }
                  ],
                  "data": "${datum}",
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new Datum!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "blackmailazure.createDatum",
                        "parameters": [
                          {
                            "name": "Datum",
                            "value": "${datum}"
                          }
                        ],
                        "then": [
                          {
                            "result": "${datum}",
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Blackmail.Models.Blackmailazure.Datum",
                  "name": "form0",
                  "type": "template-form",
                  "visible": "${datum != null}"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "datum",
        "type": "set",
        "value": "new Blackmail.Models.Blackmailazure.Datum(){}",
        "valueType": "Blackmail.Models.Blackmailazure.Datum"
      }
    ]
  },
