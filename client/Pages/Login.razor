@page "/login"
@layout LoginLayout
@inherits Blackmail.Pages.LoginComponent

@using Radzen
@using Radzen.Blazor
@using Blackmail.Models.AzureBlackmail
@using Microsoft.AspNetCore.Identity;
@using Blackmail.Models
@using Microsoft.AspNetCore.Authorization


<RadzenContent Container="main">
  <ChildContent>
    <RadzenHeading Size="H1" Text="Login">
    </RadzenHeading>
    <div class="row">
      <div class="col-md-12">
        <RadzenLogin AllowResetPassword="false" Register="@Login0Register">
        </RadzenLogin>
      </div>
    </div>
  </ChildContent>
</RadzenContent>
